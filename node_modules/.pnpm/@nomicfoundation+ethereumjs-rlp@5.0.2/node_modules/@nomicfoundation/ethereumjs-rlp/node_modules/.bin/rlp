#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules/@nomicfoundation/ethereumjs-rlp/bin/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules/@nomicfoundation/ethereumjs-rlp/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules/@nomicfoundation/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules/@nomicfoundation/ethereumjs-rlp/bin/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules/@nomicfoundation/ethereumjs-rlp/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules/@nomicfoundation/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/@nomicfoundation+ethereumjs-rlp@5.0.2/node_modules:/Users/anshumantiwari/Documents/CODES/blockchains/HARDHAT/sample/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rlp" "$@"
else
  exec node  "$basedir/../../bin/rlp" "$@"
fi
